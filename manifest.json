{
    "manifest_version": 3,
    "name": "LinkedIn Profile Name Display",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/a.jpg",
        "48": "images/a.jpg",
        "128": "images/a.jpg"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  